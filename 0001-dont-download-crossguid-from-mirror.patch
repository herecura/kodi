From ead269652a0c692bf58a9784e8f7bed087fd727b Mon Sep 17 00:00:00 2001
From: BlackEagle <ike.devolder@gmail.com>
Date: Tue, 24 Oct 2017 21:16:24 +0200
Subject: [PATCH] dont download crossguid from mirror

Signed-off-by: BlackEagle <ike.devolder@gmail.com>
---
 tools/depends/target/crossguid/Makefile | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/tools/depends/target/crossguid/Makefile b/tools/depends/target/crossguid/Makefile
index 487ab2a617..d67ff5bbd2 100644
--- a/tools/depends/target/crossguid/Makefile
+++ b/tools/depends/target/crossguid/Makefile
@@ -3,7 +3,7 @@ DEPS = Makefile
 
 # lib name, version
 LIBNAME=crossguid
-VERSION=8f399e8bd4
+VERSION=8f399e8bd4252be9952f3dfa8199924cc8487ca4
 SOURCE=$(LIBNAME)-$(VERSION)
 ARCHIVE=$(SOURCE).tar.gz
 
@@ -42,7 +42,7 @@ $(PREFIX)/lib/lib$(LIBNAME).a:
 	@make .installed-$(PLATFORM)
 
 $(TARBALLS_LOCATION)/$(ARCHIVE):
-	cd $(TARBALLS_LOCATION); $(RETRIEVE_TOOL) $(RETRIEVE_TOOL_FLAGS) $(BASE_URL)/$(ARCHIVE)
+	cd $(TARBALLS_LOCATION)
 
 $(PLATFORM): $(TARBALLS_LOCATION)/$(ARCHIVE) $(DEPS)
 ifeq ($(PREFIX),)
-- 
